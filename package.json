{
  "name": "generator-react-frontend-boilerplate",
  "version": "1.3.7",
  "description": "Boilerplate for apps using React and Webpack",
  "main": "app.js",
  "keywords": [
    "react",
    "webpack",
    "browsersync",
    "babel",
    "sass"
  ],
  "bin": {
    "generate-react-frontend": "generate.js",
    "grf": "generate.js"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack --progress -d --watch",
    "test": "for file in ./test/*.js \n do \n mocha --timeout 10000 --compilers js:babel-core/register $file \n done ",
    "build": "NODE_ENV=production webpack --progress -p"
  },
  "repository": "actuallymentor/react-frontend-boilerplate",
  "author": {
    "name": "Mentor Palokaj",
    "email": "mentor@palokaj.co",
    "url": "https://github.com/actuallymentor"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actuallymentor/react-frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/actuallymentor/react-frontend-boilerplate#readme",
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chai": "^4.1.0",
    "colors": "^1.1.2",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "firebase": "^4.2.0",
    "ip": "^1.1.5",
    "mocha": "^3.5.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "xmlhttprequest": "^1.8.0"
  }
}
